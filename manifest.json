{
  "manifest_version": 3,
  "name": "Instagram Focus",
  "version": "1.0",
  "description": "Removes Explore, Reels, and suggested content from Instagram. Friends only.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.instagram.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
